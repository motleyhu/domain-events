includes:
    - phpstan.common.neon
parameters:
    resultCachePath: %tmpDir%/resultCache-src.php
    paths:
        - src
    exceptions:
        check:
            missingCheckedExceptionInThrows: true
            tooWideThrowType: true
        uncheckedExceptionClasses:
            - RuntimeException
            - LogicException
            - Doctrine\DBAL\Exception
            - Doctrine\ORM\Exception\ORMException
